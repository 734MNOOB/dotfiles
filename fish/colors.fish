# Fish colors
# Make autocompletions "invisible" (same color as for the current line)
# set fish_color_autosuggestion 362c24 362c24
set fish_color_autosuggestion brblack --dim
set fish_color_command cyan --bold
set fish_color_comment brblack --dim
set fish_color_cwd white
set fish_color_cwd_root red
set fish_color_error red
set fish_color_escape ff0
set fish_color_history_current ff0
set fish_color_host normal
set fish_color_match f0f --background=ff0
set fish_color_normal normal
set fish_color_operator cyan
set fish_color_param white
set fish_color_quote magenta
set fish_color_redirection normal
set fish_color_search_match magenta
set fish_color_status ff0
set fish_color_user ff0
set fish_color_valid_path --underline
set fish_pager_color_completion white
set fish_pager_color_description brblack --dim
set fish_pager_color_prefix normal
set fish_pager_color_progress cyan
